{
  "index.php":{
    "actions": [
      "sleep",
      "servefile"
    ],
    "sleep":{
      "duration": 2
    },
    "servefile":{
      "file": "templates/ugly/index.html",
      "process_template": true
    },
    "headers": {
      "Server": "lighttpd",
      "X-Powered-By": "PHP/5.3.7"
    }
  },
  "":{
    "actions": [
      "sleep",
      "servefile"
    ],
    "sleep":{
      "duration": 2
    },
    "servefile":{
      "file": "templates/ugly/index.html",
      "process_template": true
    },
    "headers": {
      "Server": "lighttpd",
      "X-Powered-By": "PHP/5.3.7"
    }
  },
  "login.php":{
    "actions": [
      "sleep",
      "servefile"
    ],
    "sleep":{
      "duration": 1
    },
    "servefile":{
      "file": "templates/ugly/login.html",
      "process_template": true
    },
    "headers": {
      "Server": "lighttpd",
      "X-Powered-By": "PHP/5.3.7"
    }
  },
  "admin.php":{
    "actions": [
      "sleep",
      "authorize",
      "servefile"
    ],
    "authorize": {
      "key_username": "username",
      "key_password": "password",
      "on_error": "templates/ugly/wrongpass.html",
      "user_db": "userdb.txt",
      "on_error_process_template": true
    },
    "sleep":{
      "duration": 2
    },
    "servefile":{
      "file": "templates/ugly/admin.html",
      "process_template": true
    },
    "headers": {
      "Server": "lighttpd",
      "X-Powered-By": "PHP/5.3.7"
    }
  },
  "update.php":{
    "actions": [
      "sleep",
      "catchfile",
      "servefile"
    ],
    "sleep":{
      "duration": 5
    },
    "servefile":{
      "file": "templates/ugly/update.html",
      "process_template": true
    },
    "headers": {
      "Server": "lighttpd",
      "X-Powered-By": "PHP/5.3.7"
    }
  },
  "jquery.min.js":{
    "actions": [
      "servefile"
    ],
    "servefile":{
      "file": "templates/ugly/jquery.min.js"
    },
    "headers": {
      "Server": "lighttpd"
    }
  },
  "cgi-bin/image.pl": {
    "actions": [
      "sleep",
      "servefile"
    ],
    "sleep":{
      "duration": 1
    },
    "servefile":{
      "file": [
        "image.jpg"
      ],
      "watermark": {
        "x": 5,
        "y": 5,
        "text": "%a, %d %b %Y %H:%M:%S",
        "color": [255, 255, 255]
      }
    },
    "headers": {
      "Server": "RTSP-Stream"
    }
  }
}